{
  "name": "postauto-backend",
  "version": "1.0.0",
  "description": "Backend NestJS para automação de posts",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:generate": "typeorm migration:generate -d dist/config/typeorm.config.js",
    "migration:run": "typeorm migration:run -d dist/config/typeorm.config.js",
    "migration:revert": "typeorm migration:revert -d dist/config/typeorm.config.js"
  },
  "dependencies": {
