    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/bullmq": "^10.0.1",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.3.2",
    "node-telegram-bot-api": "^0.64.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
